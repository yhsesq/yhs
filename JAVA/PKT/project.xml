<?xml version="1.0" encoding="ISO-8859-1"?>
<dialog font="Dialog bold 14" colspan="2" property="encoding=ISO-8859-1;cls=net.sf.pkt.PKTXUL" text="  Project Settings" icon="jserv.gif" modal="true" border="false">
  <label background="255,255,255" icon="project.gif" />
  <panel background="255,255,255" weightx="1" weighty="1" columns="1" top="6" left="6" bottom="6" right="6" gap="6">
    <panel background="204,204,204" columns="4" top="6" left="6" bottom="6" right="6" gap="6" border="true">
      <label text="Project Type" />
      <separator />
      <checkbox name="cb_DialogType" text="Dialog" group="prjtype" />
      <checkbox name="cb_PanelType" text="Panel" group="prjtype" />
    </panel>
    <panel background="204,204,204" columns="4" top="6" left="6" bottom="6" right="6" gap="6" border="true">
      <label text="XML Encoding" />
      <separator />
      <checkbox name="cb_UTF" text="UTF-8" group="prjtype" />
      <checkbox name="cb_ISO" text="ISO-8859-1" group="prjtype" />
    </panel>
    <panel background="204,204,255" columns="1" top="4" left="6" bottom="6" right="6" gap="3" border="true">
      <label tooltip="Build Folder" text="Project Build Path (i.e. C:/myProject/classes/ )" />
      <textfield name="tf_BuildPath" weightx="1" />
      <label tooltip="Class Name" text="Implementation (i.e. com.myCompany.myProject.myThinlet )" />
      <textfield name="tf_Class" weightx="1" />
    </panel>
    <panel background="72,72,106" weightx="1" top="4" left="4" bottom="4" gap="6" border="true">
      <button name="btn_OK" font="Dialog bold 12" text="OK" />
      <button name="btn_Cancel" font="Dialog bold 12" text="Cancel" />
    </panel>
  </panel>
</dialog>

